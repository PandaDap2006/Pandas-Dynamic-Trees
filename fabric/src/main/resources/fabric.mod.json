{
  "schemaVersion": 1,
	"id": "${id}",
	"version": "${version}",
	"name": "${name}",
	"description": "${description}",
	"authors": [
		"The Panda Oliver"
	],
	"contact": {
		"issues": "https://github.com/PandaDap2006/Pandas-Falling-Trees/issues",
		"sources": "https://github.com/PandaDap2006/Pandas-Falling-Trees"
	},
  "license": "GPL-3",
	"icon": "assets/fallingtrees/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
			"me.pandamods.fallingtrees.fabric.FallingTreesFabric"
    ],
		"client": [
			"me.pandamods.fallingtrees.fabric.FallingTreesFabricClient"
		],
		"jade": [
			"me.pandamods.fallingtrees.compat.JadePlugin"
		]
  },
  "mixins": [
		"fallingtrees.mixins.json",
    "fallingtrees-common.mixins.json"
  ],
  "depends": {
    "fabric": "*",
		"fabric-api": "*",
		"minecraft": "${compatible_fabricmc_versions}",
		"java": ">=${java_version}",
